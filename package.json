{
  "name": "trading-helper",
  "version": "2.7.1",
  "description": "",
  "scripts": {
    "glogin": "clasp login",
    "glogout": "clasp logout",
    "gcreate": "clasp create --title trading-helper-dev --type standalone --rootDir ./apps-script",
    "gedit": "clasp open -P ./apps-script",
    "gopen": "clasp open -P ./apps-script --webapp",
    "gpush": "npm run build && clasp push -P ./apps-script -f",
    "gstart": "clasp push -P ./apps-script -w -f",
    "gbuild": "node gas-build.js",
    "start": "parcel src/web/index.html --dist-dir ./apps-script",
    "report": "parcel build src/web/index.tsx --dist-dir ./parcel-bundle-reports --reporter @parcel/reporter-bundle-analyzer",
    "build": "npm run gbuild && parcel build src/web/index.html --dist-dir ./apps-script --no-cache",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\" --config ./.prettierrc",
    "prepare": "husky install",
    "test": "node --require ts-node/register --unhandled-rejections=strict test/Backtest.ts --project test/tsconfig.json"
  },
  "devDependencies": {
    "@google/clasp": "^2.4.1",
    "@parcel/reporter-bundle-analyzer": "^2.5.0",
    "@types/google-apps-script": "^1.0.47",
    "@types/google.script.client-side": "^0.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "binance-historical": "^1.5.10",
    "esbuild": "^0.14.39",
    "esbuild-gas-plugin": "^0.4.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-googleappsscript": "^1.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "author": "Bohdan Kovalov",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "5.8.3",
    "@mui/material": "5.8.3",
    "lightweight-charts": "3.8.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-error-boundary": "3.1.4",
    "react-vis": "1.11.7",
    "react-window": "1.8.7",
    "trading-helper-lib": "0.1.26-cjs",
    "browser-or-node": "^2.0.0"
  }
}
